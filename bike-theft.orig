<script>
  function iOS() {
    return [
      'iPad Simulator',
      'iPhone Simulator',
      'iPod Simulator',
      'iPad',
      'iPhone',
      'iPod'
    ].includes(navigator.platform)
    // iPad on iOS 13 detection
    || (navigator.userAgent.includes("Mac") && "ontouchend" in document)
  }
  $(function () {
    if (!iOS()) {
      $('.images-rotation').imagesRotation({interval: 400, intervalFirst: 200})
      $('.images-rotation').mouseleave(function() {
        var images = JSON.parse($(this ).attr("data-images"))
        if (images && images[0]) {
          $( "img", this ).attr("src", images[0])
        }
      })
      $('.images-rotation img').click(function() {
        $('.images-rotation').imagesRotationRemove()
      })
    }
  });
</script>

